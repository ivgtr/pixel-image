  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@vercel/node": "^1.12.1",
    "autoprefixer": "^10.4.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.4.5",
